<br><br><br><br>
<div class="container">

				<p>
				  <strong>First name:</strong>
				  <%= @profile.first_name %>
				</p>
				
				<p>
				  <strong>Last name:</strong>
				  <%= @profile.last_name %>
				</p>
				
				<p>
				  <strong>Company name:</strong>
				  <%= @profile.company_name %>
				</p>
				
				<p>
				  <strong>Address:</strong>
				  <%= @profile.address %>
				</p>
				
				<p>
				  <strong>County:</strong>
				  <%= @profile.county %>
				</p>
				
				<p>
				  <strong>Zip:</strong>
				  <%= @profile.zip %>
				</p>
				
				<p>
				  <strong>Phone1:</strong>
				  <%= @profile.phone1 %>
				</p>
				
				<p>
				  <strong>Phone2:</strong>
				  <%= @profile.phone2 %>
				</p>
				
				<p>
				  <strong>Email:</strong>
				  <%= @profile.email %>
				</p>
				
				<p>
				  <strong>City:</strong>
				  <%= @profile.city.name %>
				</p>
				
				<p>
				  <strong>Web:</strong>
				  <%= link_to @profile.web, @profile.web, {target: "_blank"} %>
				</p>
				
				<%= link_to 'Edit', edit_profile_path(@profile), :class => 'btn btn-mini btn-primary' %> |
				<%= link_to 'Back', profiles_path, :class => 'btn btn-mini btn-default' %>
		</div>


</div>